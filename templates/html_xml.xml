<templateSet group="html/xml">
  <template name="CD" value="&lt;![CDATA[&#10;$SELECTION$&#10;]]&gt;" description="Surround with CDATA section" toReformat="false" toShortenFQNames="false" deactivated="true">
    <context>
      <option name="COMPLETION" value="false" />
      <option name="JSP" value="true" />
    </context>
  </template>
  <template name="li" value="&lt;li&gt;$SELECTION$&lt;/li&gt;" description="" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="P" value="&lt;p&gt;$SELECTION$&lt;/p&gt;" description="" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="CSS" value="true" />
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="H1" value="&lt;h1&gt;$SELECTION$&lt;/h1&gt;" description="" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="T2" value="&lt;$TAG$&gt;$SELECTION$&lt;/$TAGNAME$&gt;" description="Surround with &lt;tag&gt;&lt;/tag&gt; in HTML/JSP" toReformat="false" toShortenFQNames="false" deactivated="true">
    <variable name="TAG" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="TAGNAME" expression="firstWord(TAG)" defaultValue="&quot;&quot;" alwaysStopAt="false" />
    <context>
      <option name="COMPLETION" value="false" />
      <option name="JAVA_CODE" value="false" />
      <option name="JAVA_COMMENT" value="false" />
      <option name="JAVA_STRING" value="false" />
      <option name="JSP" value="true" />
    </context>
  </template>
  <template name="H2" value="&lt;h2&gt;$SELECTION$&lt;/h2&gt;" description="" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="H3" value="&lt;h3&gt;$SELECTION$&lt;/h3&gt;" description="" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="EM" value="&lt;em&gt;$SELECTION$&lt;/em&gt;" description="" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="CSS" value="true" />
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="Strong" value="&lt;strong&gt;$SELECTION$&lt;/strong&gt;" description="" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="CSS" value="true" />
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="AHREF" value="&lt;a href=&quot;#&quot;&gt;$SELECTION$&lt;/a&gt;" description="" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="CSS" value="true" />
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="Img" value="&lt;img src=&quot;&quot; title=&quot;&quot; alt=&quot;&quot;$SELECTION$ /&gt;" description="" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="CSS" value="true" />
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="Tag" value="&lt;$TAG$&gt;$SELECTION$&lt;/$TAGNAME$&gt;" description="" toReformat="false" toShortenFQNames="false">
    <variable name="TAG" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="TAGNAME" expression="firstWord(TAG)" defaultValue="&quot;&quot;" alwaysStopAt="false" />
    <context>
      <option name="COMPLETION" value="false" />
      <option name="HTML" value="true" />
      <option name="JAVA_CODE" value="false" />
      <option name="JAVA_COMMENT" value="false" />
      <option name="JAVA_STRING" value="false" />
      <option name="JSP" value="true" />
      <option name="JSX_HTML" value="true" />
    </context>
  </template>
  <template name="div" value="&lt;div&gt;$SELECTION$&lt;/div&gt;" description="" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
</templateSet>